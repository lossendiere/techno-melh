                    "margin-right": "2rem"
                  },
                  "& when (@reverseContent)": {
                    "margin-left": "2rem"
                  }
                }
              }
            },
            ".custom-link": {
              "position": "relative",
              "margin-left": "10px",
              "&::before, &::after": {
                "content": "''",
                "position": "absolute",
                "display": "block"
              },
              "&::after": {
                "top": "35%",
                "width": "6px",
                "height": "6px",
                "right": "0",
                "border": "1px solid @linksColor",
                "border-bottom": "none",
                "border-left": "none",
                "transition": "all 0.2s ease-in",
                "transform": "rotate(45deg)"
              },
              "&::before": {
                "top": "50%",
                "background-color": "@linksColor",
                "height": "1px",
                "left": "0",
                "opacity": "0",
                "transition": "all 0.2s ease-in",
                "width": "0"
              }
            },
            ".card-wrapper:hover": {
              ".custom-link::before": {
                "opacity": "1",
                "width": "30px"
              },
              ".custom-link::after": {
                "right": "-30px"
              }
            },
            ".link": {
              "color": "@linksColor !important"
            }
